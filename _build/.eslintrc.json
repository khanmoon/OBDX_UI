{
  "env": {
    "browser": true
  },
  "globals": {
    "define": false,
    "require": false,
    "google": false,
    "Promise": false
  },
  "plugins": ["extra-rules", "no-storage", "requirejs"],
  "extends": "eslint:recommended",
  "rules": {
    "valid-jsdoc": 0,
    "require-jsdoc": 0,
    "no-undef-init": 2,
    "no-multi-spaces" : 2,
    "no-continue" : 2,
    "no-array-constructor" : 2,
    "no-undef": 2,
    "array-callback-return": 2,
    "complexity": ["error", { "max": 30 }],
    "no-unneeded-ternary": 2,
    "no-else-return" : 2,
    "no-useless-call": 2,
    "no-unused-expressions": 2,
    "no-unmodified-loop-condition": 2,
    "no-use-before-define": 2,
    "no-new-func": 2,
    "strict": 2,
    "no-multiple-empty-lines": 2,
    "no-mixed-operators": 2,
    "no-trailing-spaces": 2,
    "no-useless-concat": 2,
    "no-console": 2,
    "no-useless-escape": 0,
    "no-inline-comments": 2,
    "for-direction": 2,
    "no-alert": 2,
    "guard-for-in": 2,
    "no-empty-function": 2,
    "no-eval": 2,
    "no-multi-str": 2,
    "no-eq-null": 2,
    "eqeqeq": 2,
    "comma-dangle": 2,
    "semi": 2,
    "yoda": 2,
    "no-script-url": 2,
    "no-self-compare" : 2,
    "extra-rules/no-commented-out-code": 2,
    "quotes": ["error", "double"],
    "object-property-newline": 2,
    "dot-notation": 2,
    "no-storage/no-browser-storage": 2,
    "requirejs/no-js-extension": 2,
    "no-new-object" : 2,
    "no-lonely-if" : 2,
    "no-lone-blocks" : 2,
    "no-useless-return" : 2,
    "no-prototype-builtins" : 2,
    "getter-return" : 2,
    "no-implicit-globals" : 2,
    "no-self-compare" : 2,
    "no-shadow-restricted-names" : 2
  }
}
