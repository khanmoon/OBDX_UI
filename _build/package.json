{
  "name": "obdx-build-tool",
  "version": "18.2.0",
  "description": "A tool to build the GUI, require optimization, minify JS, CSS, HTML and compile SASS to CSS and update the resource path variables dynamically",
  "author": "Ravindra Singh",
  "private": true,
  "scripts": {
    "jsdoc": "rd /s /q ..\\jsdocs & .\\node_modules\\.bin\\jsdoc -c jsdoc.json --readme jsdoc-conf/README.md",
    "jsdoc-nix": "./node_modules/.bin/jsdoc -c jsdoc.json --readme jsdoc-conf/README.md",
    "production": "npm install --only=production"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "docdash": "^0.4.0",
    "eslint-plugin-extra-rules": "^0.8.1",
    "eslint-plugin-no-storage": "^1.0.2",
    "eslint-plugin-requirejs": "^3.1.1",
    "esprima": "^4.0.0",
    "file-system": "^2.2.2",
    "grunt": "*",
    "grunt-cache-bust": "*",
    "grunt-concurrent": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-htmlmin": "*",
    "grunt-contrib-requirejs": "*",
    "grunt-contrib-uglify": "*",
    "grunt-css-clean": "*",
    "grunt-css-mqpacker": "*",
    "grunt-cwebp": "*",
    "grunt-eslint": "*",
    "grunt-newer": "*",
    "grunt-sass": "*",
    "grunt-string-replace": "*",
    "jsdoc": "^3.5.5",
    "matchdep": "*",
    "multi-glob": "^1.0.1",
    "node-sass": "^4.11.0",
    "rimraf": "*",
    "time-grunt": "*",
    "typo-js": "^1.0.3"
  },
  "devDependencies": {
    "chalk": "^2.1.0",
    "execa": "^0.8.0",
    "js-beautify": "^1.7.3",
    "listr": "^0.12.0",
    "node-notifier": "^5.2.1"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
